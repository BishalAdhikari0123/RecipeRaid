{
  "name": "recipe-raid-co-op",
  "version": "1.0.0",
  "description": "Gamified cooking co-op raids application",
  "scripts": {
    "install:all": "cd client && npm install && cd ../server && npm install",
    "dev:server": "cd server && npm run dev",
